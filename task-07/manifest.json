{
  "name":"SkySync",
  "description":"Reading Between the Clouds for You!",
  "version": "0.0.1",
  "manifest_version": 3,
  "action":{
    "default_popup":"/popup.html",
 "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; https://api.openweathermap.org/ https://openweathermap.org/"
  },

    "default_icon": "images/logo.png"
  },

  "icons": {
  
   "128": "images/logo.png"
  },

  "permissions": ["geolocation"]
}
